class Solution:
    def getLastDigit(self, a, b):
        # code here 
        a=int(a)
        b=int(b)
        if(a==0):
            return 0
        if(b==0):
            return 1
        if((b%4)==0):
            res=4;
        else:
            res=b%4
        num=pow(a,res)
        l=num%10
        return l
